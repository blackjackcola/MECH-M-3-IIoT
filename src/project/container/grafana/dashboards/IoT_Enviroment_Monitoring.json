          "query": "import \"strings\"\r\n\r\ndata =\r\n    from(bucket: \"iot_monitoring\")\r\n        |> range(start: -5d)\r\n        |> filter(fn: (r) => r.topic =~ /\\/status$/)\r\n        |> filter(fn: (r) =>\r\n            r._field == \"device_id\" or\r\n            r._field == \"status\" or\r\n            r._field == \"timestamp\"\r\n        )\r\n        |> group(columns: [\"topic\"])\r\n\r\npivoted =\r\n    data\r\n        |> pivot(\r\n            rowKey: [\"topic\"],\r\n            columnKey: [\"_field\"],\r\n            valueColumn: \"_value\"\r\n        )\r\n\r\npivoted\r\n    |> map(fn: (r) => ({\r\n        device_id: r.device_id,\r\n        status:    r.status,\r\n        last_seen: r.timestamp\r\n    }))\r\n    |> keep(columns: [\"device_id\", \"status\", \"last_seen\"])\r\n",
